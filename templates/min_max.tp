//% T = varargin{:}; Ts = contain_type(T);
//% if is_dense(T) && is_real(Ts) && ~is_Bool(Ts)
inline const @Ts@ max(@T@_I v)
{
    return max_v(v.ptr(), v.size());
}
//% elseif is_Dvec(T) && is_real(Ts) && ~is_Bool(Ts)
inline const @Ts@ max(@T@_I v)
{
    return max_v(v.ptr(), v.size(), v.step());
}
