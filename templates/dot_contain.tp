//% types = {
//%     'VecLlong', 'VecLlong';
//%     'VecDoub', 'VecDoub';
//%     'VecComp', 'VecComp';
//% };
//%---------------------
//% [T1, T2] = varargin{:};
//% T = promo_type(contain_type(T1), contain_type(T2));
//% if is_dense(T1) && is_dense(T2)
inline @T@ dot(@T1@_I v1, @T2@_I v2)
{
#ifdef SLS_CHECK_SHAPE
    if (!shape_cmp(v1, v2))
        SLS_ERR("wrong shape!");
#endif
    return dot_vv(v1.ptr(), v2.ptr(), v2.size());
}
//% end
