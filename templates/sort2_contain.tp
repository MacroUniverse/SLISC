void sort2(@T@_IO v, @T1@_IO v1)
{
#ifdef SLS_CHECK_BOUNDS
	if (!shape_cmp(v, v1))
		SLS_ERR("wrong shape!");
#endif
	sort2_vv(v.ptr(), v1.ptr(), v.size());
}
