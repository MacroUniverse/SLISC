//% [T, T1] = varargin{:};
//% Ts1 = contain_type(T1);
inline void mod_vvs(@T@ *v, const @T1@ *v1, @Ts1@_I s, Long_I N)
{
    for (Long i = 0; i < N; ++i)
        v[i] = mod(v1[i], s);
}

inline void mod_vs(@T1@ *v, @Ts1@_I s, Long_I N)
{
    for (Long i = 0; i < N; ++i)
        v[i] = mod(v[i], s);
}
