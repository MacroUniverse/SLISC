void operator+=(@T1@_O z, @T2@_I x)
{
    z += (@T@)x;
}

void operator-=(@T1@_O z, @T2@_I x)
{
    z -= (@T@)x;
}

void operator*=(@T1@_O z, @T2@_I x)
{
    z *= (@T@)x;
}

void operator/=(@T1@_O z, @T2@_I x)
{
    z /= (@T@)x;
}
