//%----------------------------------
//% [T1,T2] = varargin{:};
//% if is_real(T1);
//%     T = rm_comp(T1);
//% else
//%     T = T1;
//% end
inline void operator+=(@T1@_O z, @T2@_I x)
{
    z += (@T@)x;
}

inline void operator-=(@T1@_O z, @T2@_I x)
{
    z -= (@T@)x;
}

inline void operator*=(@T1@_O z, @T2@_I x)
{
    z *= (@T@)x;
}

inline void operator/=(@T1@_O z, @T2@_I x)
{
    z /= (@T@)x;
}
