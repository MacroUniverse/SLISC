//%------------------------------
//% [T, Ts1] = varargin{:};
//% Ts = value_type(T);
inline void copy(@T@_O v, const std::initializer_list<@Ts1@> &v1)
{
    const @Ts1@ *p1 = v1.begin();
    Long N = v1.size();
#ifdef SLS_CHECK_SHAPES
    if (v.size() != N)
        SLS_ERR("wrong shape!");
#endif
    for (Long i = 0; i < N; ++i)
        v[i] = p1[i];
}
