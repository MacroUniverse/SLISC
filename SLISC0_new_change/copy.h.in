#ifdef SLS_USE_QUAD_MATH
//% end
inline void assign(@T@_O v, ...)
{
    va_list args;
    va_start(args, v);
    Long N = v.size();
    for (Long i = 0; i < N; ++i)
        v[i] = va_arg(args, @Ts@);
    va_end(args);
}

inline void assign2(@T@_IO v, Long_I ind, Long_I N, ...)
{
    va_list args;
    va_start(args, N);
    for (Long i = ind; i < ind+N; ++i)
        v[i] = va_arg(args, @Ts@);
    va_end(args);
}
//% if is_Qdoub(Ts) || is_Qcomp(Ts)
#endif
