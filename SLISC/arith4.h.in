{ add_eq_vs(v.p(), s, v.size(), v.step()); }

inline void operator-=(@T@_IO v, @Ts@_I s)
{ sub_eq_vs(v.p(), s, v.size(), v.step()); }

inline void operator*=(@T@_IO v, @Ts@_I s)
