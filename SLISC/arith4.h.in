{ add_eq_vs(v.data(), s, v.size()); }

inline void operator-=(@T@_IO v, @Ts@_I s)
{ sub_eq_vs(v.data(), s, v.size()); }

inline void operator*=(@T@_IO v, @Ts@_I s)
{ mul_eq_vs(v.data(), s, v.size()); }

inline void operator/=(@T@_IO v, @Ts@_I s)
{ div_eq_vs(v.data(), s, v.size()); }
