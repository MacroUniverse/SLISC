        add_eq_vs(&v(0,j), s, v.n0());
}

inline void operator-=(@T@_IO v, @Ts@_I s)
{
    for (Long j = 0; j < v.n1(); ++j)
        sub_eq_vs(&v(0,j), s, v.n0());
}

inline void operator*=(@T@_IO v, @Ts@_I s)
{
    for (Long j = 0; j < v.n1(); ++j)
        mul_eq_vs(&v(0,j), s, v.n0());
}

inline void operator/=(@T@_IO v, @Ts@_I s)
{
    for (Long j = 0; j < v.n1(); ++j)
        div_eq_vs(&v(0,j), s, v.n0());
}
