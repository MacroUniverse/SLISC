template <class T>
inline void operator+=(vector<vector<T>> &v, const T &s)
{
    for (auto &v1 : v)
        for (auto &e : v1)
            e += s;
}

template <class T>
inline void operator-=(vector<vector<T>> &v, const T &s)
{
    for (auto &v1 : v)
        for (auto &e : v1)
            e -= s;
}

template <class T>
inline void operator*=(vector<vector<T>> &v, const T &s)
{
    for (auto &v1 : v)
        for (auto &e : v1)
            e *= s;
}

