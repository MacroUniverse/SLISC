// parse a container of numbers from a string (elements can be empty)
// str = v[0] + sep + v[1] + sep + ... v.back()
//% tem('parse2', {
//%   'vector'        'Llong' 'Str'    '" "'
//%   'vector'        'Llong' 'Str32'  'U" "'
//%   'set'           'Llong' 'Str'    '" "'
//%   'set'           'Llong' 'Str32'  'U" "'
//%   'unordered_set' 'Llong' 'Str'    '" "'
//%   'unordered_set' 'Llong' 'Str32'  'U" "'
//% });
//%----------------------------
//% [Tv, Ts, Tstr, sep] = varargin{:};
//% if strcmp(Tv, 'set') || strcmp(Tv, 'unordered_set')
//%     add = 'insert';
//% elseif strcmp(Tv, 'vector')
//%     add = 'push_back';
//% else
//%     error(['parse: not implemented: ' Tv]);
//% end
inline void parse(@Tv@<@Ts@> &v, @Tstr@_I str, @Tstr@_I sep = @sep@)
{
	Long ind0 = 0; v.clear();
	if (str.empty()) return;
	while (1) {
		Long ind1 = str.find(sep, ind0);
		if (ind1 < 0) break;
		v.@add@(str2int(str.substr(ind0, ind1-ind0)));
		ind0 = ind1 + 1;
	}
	v.@add@(str2int(str.substr(ind0)));
}

inline void join(@Tstr@_O str, const @Tv@<@Ts@> &v, @Tstr@_I sep = @sep@)
{
	str.clear();
	if (v.empty()) return;
	for (auto &e : v)
		str += to_string(e) + sep;
	str.resize(str.size() - sep.size());
}
