// convert a number to chinese
inline Str num2chinese(Long num) {
	map<Long, Str> digits = {
		{0, u8"零"}, {1, u8"一"}, {2, u8"二"}, {3, u8"三"}, {4, u8"四"},
		{5, u8"五"}, {6, u8"六"}, {7, u8"七"}, {8, u8"八"}, {9, u8"九"},
	};
	map<Long, Str> units = {
		{1, u8"十"}, {2, u8"百"}, {3, u8"千"}, {4, u8"万"},
	};

	Str result = "";

	if (num >= 100000 || num < 0)
		SLS_ERR("not supported yet!");

	if (num >= 10000) {
		Long num1 = num / 10000;
		num %= 10000;
		result += digits[num1] + units[4];
		if (num > 0 && num < 1000)
			result += digits[0];
	}

	if (num >= 1000) {
		Long num1 = num / 1000;
		num %= 1000;
		result += digits[num1] + units[3];
		if (num > 0 && num < 100)
			result += digits[0];
	}

	if (num >= 100) {
		Long num1 = num / 100;
		num %= 100;
		result += digits[num1] + units[2];
		if (num > 0 && num < 10)
			result += digits[0];
	}

	if (num >= 10) {
		Long num1 = num / 10;
		num %= 10;
		if (num1 != 1 || !result.empty())
			result += digits[num1];
		result += units[1];
	}

	if (num > 0 || result.empty())
		result += digits[num];

	if (result.substr(0, 2) == u8"一十")
		result = result.substr(1);
	return result;
}

// parse a container of strings from a string (elements can be empty)
// str = v[0] + sep + v[1] + sep + ... v.back()
//% tem('parse', {
//%   'vector'        'Str'    '" "'
//%   'vector'        'Str32'  'U" "'
//%   'set'           'Str'    '" "'
//%   'set'           'Str32'  'U" "'
//%   'unordered_set' 'Str'    '" "'
//%   'unordered_set' 'Str32'  'U" "'
//% });
//%----------------------------
//% [Tv, Ts, sep] = varargin{:};
//% if strcmp(Tv, 'set') || strcmp(Tv, 'unordered_set')
//%     add = 'insert';
//% elseif strcmp(Tv, 'vector')
//%     add = 'push_back';
//% else
//%     error(['parse: not implemented: ' Tv]);
//% end
inline void parse(@Tv@<@Ts@> &v, @Ts@_I str, @Ts@_I sep = @sep@)
{
	Long ind0 = 0; v.clear();
	if (str.empty()) return;
	while (1) {
		Long ind1 = str.find(sep, ind0);
		if (ind1 < 0) break;
		v.@add@(str.substr(ind0, ind1-ind0));
		ind0 = ind1 + 1;
	}
	v.@add@(str.substr(ind0));
}

inline void join(@Ts@_O str, const @Tv@<@Ts@> &v, @Ts@_I sep = @sep@)
{
	str.clear();
	if (v.empty()) return;
	for (auto &e : v)
		str += e + sep;
	str.resize(str.size() - sep.size());
}
