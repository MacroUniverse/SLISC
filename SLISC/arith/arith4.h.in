inline void operator+=(@T@_O v, @T1@_I v1)
{
	assert_same_shape(v, v1);
	add_eq_vv(&v[0], &v1[0], v1.size());
}

inline void operator-=(@T@_O v, @T1@_I v1)
{
	assert_same_shape(v, v1);
	sub_eq_vv(&v[0], &v1[0], v1.size());
}

