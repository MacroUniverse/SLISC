//% elseif ndim(T) == 2 && ndim(T1) == 2
inline void operator+=(@T@_O v, @T1@_I v1) // slow!
{
//% req('assert_same_shape', {T, T1});
	assert_same_shape(v, v1);
	for (Long j = 0; j < v.n1(); ++j)
		for (Long i = 0; i < v.n0(); ++i)
			v(i, j) += v1(i, j);
}

inline void operator-=(@T@_O v, @T1@_I v1) // slow!
{
	assert_same_shape(v, v1);
	for (Long j = 0; j < v.n1(); ++j)
		for (Long i = 0; i < v.n0(); ++i)
			v(i, j) -= v1(i, j);
}

inline void operator*=(@T@_O v, @T1@_I v1) // slow!
{
	assert_same_shape(v, v1);
	for (Long j = 0; j < v.n1(); ++j)
		for (Long i = 0; i < v.n0(); ++i)
			v(i, j) *= v1(i, j);
}

inline void operator/=(@T@_O v, @T1@_I v1) // slow!
{
	assert_same_shape(v, v1);
	for (Long j = 0; j < v.n1(); ++j)
		for (Long i = 0; i < v.n0(); ++i)
			v(i, j) /= v1(i, j);
}
