function ret = contain_type(type)
if any(strfind(type, 'Char'))
    ret = 'Char';
elseif any(strfind(type, 'Int'))
    ret = 'Int';
elseif any(strfind(type, 'Llong'))
    ret = 'Int';
elseif any(strfind(type, 'Doub'))
    ret = 'Doub';
elseif any(strfind(type, 'Comp'))
    ret = 'Comp';
elseif any(strfind(type, 'Comp'))
    ret = 'Comp';
elseif any(strfind(type, 'Comp'))
    ret = 'Comp';
elseif any(strfind(type, 'Comp'))
    ret = 'Comp';
else
    err('not implemented');
end
end
